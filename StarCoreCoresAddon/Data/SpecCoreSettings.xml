	<PointsFormats>
		<!--
		Id - id of PointFormat
		OverlimitWarning - text that would be added if overlimiting some of point
		OverlimitWarningAddLineAfter="true" - should add new line if overlimiting some of point
		OverlimitWarningAddLineBefore="true" - should add new line if overlimiting some of point
		Format="{0}/{1} ({2})" - format of text. {0} - current points, {1} - max / min points, {2} - on grid
		NumberFormat="{0:G29}"  - format of number
		WarningSign="!!!" - text that shows, when {2} more/less {1}
		Visible="true" - show in detailed info
		VisibleIfAllZero="false" - show if "0/0 (0)"
		VisibleIfInfinity="true" - show if "?/Infinity (?)"
		Color format is #AARRGGBB
		  -->
  
	<PointFormat Id="DefaultLessOrEqual"
		Format="[Color=#FF6FFF72]{PointName}:{Current}/{Max}[/Color]"  
		FormatOverlimiting="[Color=#FFFF0000]{PointName}:{Current}/{Max} OVER LIMIT[/Color]" 
		FormatPossibleOverlimiting="[Color=#FFFF9100]{PointName}:{Current}/{Max} Over Limit[/Color]"
		NumberFormat="{0:G29}" 
		Visible="true"
		VisibleIfAllZero="false"
		VisibleIfInfinity="false"  />
	<PointFormat Id="DefaultSumLessOrEqual" 
		Format="[Color=#FF6FFF72]{PointName}:{Current}/{Max}[/Color]" 
		FormatOverlimiting="[Color=#FFFF0000]{PointName}:{Current}/{Max} OVER LIMIT[/Color]" 
		FormatPossibleOverlimiting="[Color=#FFFF9100]{PointName}:{Current}/{Max} Over Limit[/Color]"
		NumberFormat="{0:G29}" 
		Visible="true"
		VisibleIfAllZero="false"
		VisibleIfInfinity="false" />	
	<PointFormat Id="DefaultMoreOrEqual" 
		Format="[Color=#FF6FFF72]{PointName}:{Current}/{Max}[/Color]" 
		FormatOverlimiting="[Color=#FFFF0000]{PointName}:{Current}/{Min} UNDER LIMIT[/Color]" 
		FormatPossibleOverlimiting="[Color=#FFFF9100]{PointName}:{Current}/{Max} Under Minimum[/Color]"
		NumberFormat="{0:G29}" 
		Visible="true"
		VisibleIfAllZero="false"
		VisibleIfInfinity="false" />	
	<PointFormat Id="DefaultProperty" 
		Format="[Color=#FFCD57FF]{PointName}:{Max}[/Color]" 
		FormatOverlimiting="" 
		FormatPossibleOverlimiting="" 
		NumberFormat="{0:G3}" 
		Visible="true"
		VisibleIfAllZero="false"
		VisibleIfInfinity="false" />	
	<PointFormat Id="DefaultUpgradePoint" 
		Format="[Color=#FF54AAFF]{PointName}:{Max}[/Color]" 
		FormatOverlimiting="" 
		FormatPossibleOverlimiting="" 
		NumberFormat="{0:G29}" 
		Visible="true"
		VisibleIfAllZero="false"
		VisibleIfInfinity="false" />
	</PointsFormats>
	<Points>
		<!-- 
		int Id;
		string FormatId - id of PointsFormats. When it is npot found, used default.
		string Name - translatable name of point
		float DisplayOrder - order in DetailedInfo
		string UnitName - translatable text that would be added after point info
		
		PointTypes:
			Property - just a value. Not used in checks at all
			SumLessOrEqual (default) - used in SpecBlock & Limited blocks. Values from all limited blocks are summed. Example: "PowerPoints"
			LessOrEqual - used in SpecBlock & Limited blocks. Block value must be less than SpecCore value. Example: "MaxAllowedTier", less that 9000 PCU
			MoreOrEqual - used in SpecBlock (Limited blocks doesn't support it). Block value must be more than SpecCore value. Example: "MaxAllowedTier", more that 2 grids		
			
			
		DefaultSpecCoreValue - would be used for 
		ActivationError - text that shows when that point overlimits
		
		
		Restricted Point IDS:
		
		TYPE_MAX_Internal_SmallGrids = -5
        TYPE_MAX_Internal_LargeGrids = -6
        TYPE_MAX_GRIDS = -7
        TYPE_MAX_PCU = -8
        TYPE_MAX_BLOCKS = -9
        
        TYPE_MIN_Internal_SmallGrids = -15
        TYPE_MIN_Internal_LargeGrids = -16
        TYPE_MIN_GRIDS = -17
        TYPE_MIN_PCU = -18
        TYPE_MIN_BLOCKS = -19
		Warhead
	-->
		<Point PointId="Internal_MaxBlocks" DefaultSpecCoreValue="500" DefaultNoSpecCoreValue="500" NId="-9" DisplayOrder="-3" Behavior="LessOrEqual" ActivationError="Over Maximum Blocks Limit!" Name="Max Blocks"/>
		<Point PointId="Points_Mobility" DefaultSpecCoreValue="40" DefaultNoSpecCoreValue="2" NId="103" DisplayOrder="-2" Behavior="SumLessOrEqual" Name="Mobility Points"/>
